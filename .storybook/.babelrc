{
  "presets": ["babel-preset-gatsby"],
  "plugins": [
    [
      "react-css-modules",
      {
        "generateScopedName": "[local]___[hash:base64:5]",
        "filetypes": {
          ".scss": {
            "syntax": "postcss-scss",
            "plugins": [
              [
                "postcss-nested",
                {
                  "preserveEmpty": true
                }
              ]
            ]
          }
        }
      }
    ]
  ]
}
